package Paket2;

public class Vezbi {

	public static void main(String[] args) {
		double a = 0.2;
		double b = 0.3;
		String name = "Правоаголник";
		String value1 = "Плоштина";
		int index = 2222;
		
		System.out.println(name + " има страни a = " +
		(int)(a*100) + "cm и b = " + (int)(b*10) + "cm. " +
				value1 + "та изнесува " + (int)((a*100) * (int)(b*10)) + "cm.");
		
	}
}
