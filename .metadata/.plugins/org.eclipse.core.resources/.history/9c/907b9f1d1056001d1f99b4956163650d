package Paket2;

public class Vezbi {

	public static void main(String[] args) {
		String firstName = "Илче";
		String secondName = "Наумовски";
		int index = 2222;
		
		System.out.println("Здраво, јас сум " + 
		firstName + " " + secondName + ". " + 
				"Мојот број на индекс е " + index);
		
	}
}
